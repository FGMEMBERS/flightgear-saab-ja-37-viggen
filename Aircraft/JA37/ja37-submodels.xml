<PropertyList>

  <submodel>
    <name>flaresR</name>
    <model>Aircraft/JA37/Models/Effects/flare.xml</model>
    <trigger>ai/submodels/submodel[0]/flare-release</trigger><!-- ai/submodels/submodel[0]/flare-release -->
    <speed>600.0</speed>   <!-- ft/sec -->
