    <delay>0.15</delay>
    <count>-1</count>
    <x-offset>  3.0</x-offset>
    <y-offset> -2.5</y-offset>
    <z-offset>  1.2</z-offset>
    <pitch-offset>0</pitch-offset>
    <life>2.0</life>
    <wind>false</wind>
    <weight>0.1</weight>
    <cd>0.193</cd>
    <eda>0.00136354</eda>
  </submodel>

