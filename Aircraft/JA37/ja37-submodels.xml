    <impact>true</impact>
    <impact-report>sim/ai/aircraft/impact/cannon</impact-report>
    <submodel-path>Aircraft/JA37/Models/Armament/Weapons/KCA/bullet-submodel.xml</submodel-path>
  </submodel>


  <submodel>
    <name>gunsmoke</name>
