<?xml version="1.0"?>

<PropertyList>

  <submodel>
    <name>flares</name>
    <model>Aircraft/JA37/Models/Effects/flare.xml</model>
    <trigger>sim/multiplay/generic/int[7]</trigger><!-- ai/submodels/submodel[0]/flare-release -->
    <speed>100.0</speed>   <!-- ft/sec -->
    <repeat>true</repeat>
    <delay>0.75</delay>
    <count>40</count>
    <x-offset>5.0</x-offset>
    <y-offset>2.0</y-offset>
    <z-offset>0.1</z-offset>
    <yaw-offset>90.0</yaw-offset>
    <life>5</life>
    <buoyancy>8</buoyancy>
    <eda>1.0</eda>
  </submodel>

