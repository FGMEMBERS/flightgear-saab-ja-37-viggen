<terrain>
   <name>terrain</name>
    <reference-dist>1.0</reference-dist>
    <max-dist>2.0</max-dist>
   <mode>single</mode>
   <type>avionics</type>
   <path>Sounds/lowterrain.wav</path>
   <condition>
    <property>instrumentation/annunciators/gpws/terrain-alert</property>
   </condition>
   <volume>
    <property>instrumentation/annunciators/gpws/volume</property>
   </volume>
  </terrain>  
  
<roll>
   <name>roll</name>
   <mode>looped</mode>
   <path>Sounds/roll.wav</path>
   <condition>
    <or>
     <property>/gear/gear[0]/wow</property>
     <property>/gear/gear[1]/wow</property>
     <property>/gear/gear[2]/wow</property>
    </or>
   </condition>
   <volume>
    <property>/velocities/airspeed-kt</property>
    <type>log</type>
    <factor>0.025</factor>
   </volume>
  </roll>  
  
<wind>
			<name>wind</name>
			<mode>looped</mode>
			<path>Sounds/wind.wav</path>
			<condition>
				<equals>
					<property>sim/current-view/view-number</property>
					<value>0</value>
				</equals>
				<greater-than>
					<property>canopy/position-norm</property>
					<value>0</value>
				</greater-than>
			</condition>
			<volume>
				<property>velocities/airspeed-kt</property>
				<factor>0.001</factor>
			</volume>
		</wind>

<air-brakes>
			<name>air-brakes</name>
			<mode>looped</mode>
			<path>Sounds/air-brakes.wav</path>
			<condition>
				<equals>
					<property>sim/current-view/view-number</property>
					<value>0</value>
				</equals>
				<greater-than>
					<property>surface-positions/speedbrake-pos-norm</property>
					<value>0</value>
				</greater-than>
			</condition>
			<volume>
				<property>velocities/airspeed-kt</property>
				<factor>0.001</factor>
			</volume>
			<volume>
				<property>surface-positions/speedbrake-pos-norm</property>
				<factor>1.5</factor>
			</volume>
		</air-brakes>

  <cannon>
   <name>cannon</name>
   <path>Sounds/M61A1.wav</path>
   <property>controls/armament/trigger</property>
   <mode>looped</mode>
   <position>
    <x>-1.0</x>
    <y>0.8</y>
    <z>0.0</z>
   </position>
  </cannon>
  
<aircraft-crash>
		<name>aircraft-crash</name>
		<path>Sounds/aircraft-explode.wav</path>
		<mode>once</mode>
		<condition>
			<equals>
				<property>sounds/aircraft-crash/on</property>
				<value>1</value>
			</equals>
		</condition>
		<volume>
			<factor>1</factor>
		</volume>
	</aircraft-crash>

	<aircraft-water-crash>
		<name>aircraft-water-crash</name>
		<path>Sounds/aircraft-water-crash.wav</path>
		<mode>once</mode>
		<condition>
			<equals>
				<property>sounds/aircraft-water-crash/on</property>
				<value>1</value>
			</equals>
		</condition>
		<volume>
			<factor>1</factor>
		</volume>
	</aircraft-water-crash>   
</fx>
 
</PropertyList>
