<stallw>
		<name>stallwarning</name>
		<mode>looped</mode>
		<path>Sounds/stallwarning.wav</path>
		<condition>
  		<greater-than>
  			<property>orientation/alpha-deg</property>
  			<value>17</value>
  		</greater-than>
      <less-than>
        <property>orientation/alpha-deg</property>
        <value>23</value>
      </less-than>
  		<greater-than>
  			<property>velocities/airspeed-kt</property>
  			<value>10</value>
  		</greater-than>
  		<!--<property>/sim/current-view/internal</property>-->
  		<not>
        	<property>fdm/jsbsim/systems/crash-detect/crashed</property>
      	</not>
      	<not>
        <property>gear/gear[1]/wow</property>
        <property>gear/gear[2]/wow</property>
       </not>
		</condition>
		<volume><factor>3.5</factor></volume>
	</stallw>  
  
