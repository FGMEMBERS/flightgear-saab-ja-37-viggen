<?xml version="1.0"?>

<PropertyList>

 <path>ja37.ac</path>
 
 <model>
    <path>Models/ja37-panel.xml</path>
    <name>Panel</name>
    <offsets>
      <x-m>0</x-m>
      <y-m>0.0</y-m>
      <z-m>0</z-m>
    </offsets>
  </model>
 
<model>
    <path>Models/HUD.xml</path>
    <name>TheHUD</name>
    <offsets>
      <x-m>0</x-m>
      <y-m>0.0</y-m>
      <z-m>0</z-m>
    </offsets>
  </model> 
 
