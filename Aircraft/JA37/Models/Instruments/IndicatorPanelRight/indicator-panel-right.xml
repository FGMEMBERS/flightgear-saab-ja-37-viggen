<?xml version="1.0"?>
<PropertyList>
    <path>indicator-panel-right.ac</path>

    <!-- JA-37Di pilots Manual page 589 and forward Also bits of info scattered all over the document. -->

    <animation>
        <name>Flightstick</name>
        <object-name>indicatorLeft1</object-name>
        <type>material</type>
        <condition>
            <or>
                <and>
                    <equals>
                        <property>sim/ja37/avionics/joystick</property>
                        <value>1</value>
                    </equals>
                    <equals>
                        <property>sim/ja37/blink/ten-Hz/state</property>
                        <value>1</value>
                    </equals>
                </and>
                <property>controls/lighting/test-indicator-panels</property>
            </or>
        </condition>
        <emission>
            <red>1</red>
            <green>1</green>
            <blue>1</blue>
            <factor-prop>systems/electrical/dc-light</factor-prop>
        </emission>
    </animation>

    <animation>
        <name>a/p att</name>
        <object-name>indicatorLeft2</object-name>
        <type>material</type>
        <condition>
            <or>
                <and>
                    <equals>
                        <property>sim/ja37/avionics/autopilot</property>
                        <value>1</value>
                    </equals>
                    <equals>
                        <property>sim/ja37/blink/ten-Hz/state</property>
                        <value>1</value>
                    </equals>
                </and>
                <property>controls/lighting/test-indicator-panels</property>
            </or>
        </condition>
        <emission>
            <red>1</red>
            <green>1</green>
            <blue>1</blue>
            <factor-prop>systems/electrical/dc-light</factor-prop>
        </emission>
    </animation>

    <animation>
        <name>a/p alt</name>
        <object-name>indicatorLeft3</object-name>
        <type>material</type>
        <condition>
            <or>
                <and>
                    <equals>
                        <property>sim/ja37/avionics/autopilot</property>
                        <value>1</value>
                    </equals>
                    <equals>
                        <property>sim/ja37/blink/ten-Hz/state</property>
                        <value>1</value>
                    </equals>
                </and>
                <property>controls/lighting/test-indicator-panels</property>
            </or>
        </condition>
        <emission>
            <red>1</red>
            <green>1</green>
            <blue>1</blue>
            <factor-prop>systems/electrical/dc-light</factor-prop>
        </emission>
    </animation>

    <animation>
        <name>Fire in engine starter</name>
        <object-name>indicatorLeft4</object-name>
        <type>material</type>
        <condition>
            <property>controls/lighting/test-indicator-panels</property>
        </condition>
        <emission>
            <red>1</red>
            <green>1</green>
            <blue>1</blue>
            <factor-prop>systems/electrical/dc-light</factor-prop>
        </emission>
    </animation>

    <animation>
        <name>Pitch limiter</name>
        <object-name>indicatorLeft5</object-name>
        <type>material</type>
        <condition>
            <or>
                <less-than>
                    <property>fdm/jsbsim/systems/flight/pitch-limiter</property>
                    <value>1.0</value>
                </less-than>
                <property>controls/lighting/test-indicator-panels</property>
            </or>
        </condition>
        <emission>
            <red>1</red>
            <green>1</green>
            <blue>1</blue>
            <factor-prop>systems/electrical/dc-light</factor-prop>
        </emission>
    </animation>

    <animation>
        <name>Cabin pressure</name>
        <object-name>indicatorLeft6</object-name>
        <type>material</type>
        <condition>
            <or>
                <and>
                    <less-than>
                        <property>fdm/jsbsim/systems/flight/cabin-pressure-kpm2</property>
                        <value>0.15</value>
                    </less-than>
                    <equals>
                        <property>sim/ja37/blink/ten-Hz/state</property>
                        <value>1</value>
                    </equals>
                </and>
                <property>controls/lighting/test-indicator-panels</property>
            </or>
        </condition>
        <emission>
            <red>1</red>
            <green>1</green>
            <blue>1</blue>
            <factor-prop>systems/electrical/dc-light</factor-prop>
        </emission>
    </animation>

    <animation>
        <name>Canopy and seat</name>
        <object-name>indicatorLeft7</object-name>
        <type>material</type>
        <condition>
            <or>
                <and>
                    <or>
                        <equals>
                            <property>sim/ja37/avionics/canopyAndSeat</property>
                            <value>1</value>
                        </equals>
                        <and>
                            <not-equals>
                                <property>canopy/position-norm</property>
                                <value>0</value>
                            </not-equals>
                            <or>
                                <not-equals>
                                    <property>gear/gear[0]/wow</property>
                                    <value>1</value>
                                </not-equals>
                                <greater-than>
                                    <property>engines/engine[0]/n2</property>
                                    <value>88.5</value>
                                </greater-than>
                            </or>
                        </and>
                        <not-equals>
                            <property>fdm/jsbsim/fcs/canopy/serviceable</property>
                            <value>1</value>
                        </not-equals>
                        <not-equals>
                            <property>fdm/jsbsim/fcs/canopy/hinges/serviceable</property>
                            <value>1</value>
                        </not-equals>
                    </or>
                    <equals>
                        <property>sim/ja37/blink/ten-Hz/state</property>
                        <value>1</value>
                    </equals>
                </and>
                <property>controls/lighting/test-indicator-panels</property>
            </or>
        </condition>
        <emission>
            <red>1</red>
            <green>1</green>
            <blue>1</blue>
            <factor-prop>systems/electrical/dc-light</factor-prop>
        </emission>
    </animation>

    <animation>
        <name>Ignition sys</name>
        <object-name>indicatorLeft8</object-name>
        <type>material</type>
        <condition>
            <or>
                <and>
                    <equals>
                        <property>sim/ja37/avionics/ignitionSys</property>
                        <value>1</value>
                    </equals>
                    <equals>
                        <property>sim/ja37/blink/ten-Hz/state</property>
                        <value>1</value>
                    </equals>
                </and>
                <property>controls/lighting/test-indicator-panels</property>
            </or>
        </condition>
        <emission>
            <red>1</red>
            <green>1</green>
            <blue>1</blue>
            <factor-prop>systems/electrical/dc-light</factor-prop>
        </emission>
    </animation>

    <animation>
        <name>Start sys</name>
        <object-name>indicatorLeft9</object-name>
        <type>material</type>
        <condition>
            <or>
                <equals>
                    <property>sim/ja37/avionics/startSys</property>
                    <value>1</value>
                </equals>
                <property>controls/lighting/test-indicator-panels</property>
            </or>
        </condition>
        <emission>
            <red>1</red>
            <green>1</green>
            <blue>1</blue>
            <factor-prop>systems/electrical/dc-light</factor-prop>
        </emission>
    </animation>

    <animation>
        <name>Ext power</name>
        <object-name>indicatorLeft10</object-name>
        <type>material</type>
        <condition>
            <or>
                <equals>
                    <property>fdm/jsbsim/systems/electrical/external/supplying</property>
                    <value>1</value>
                </equals>
                <property>controls/lighting/test-indicator-panels</property>
            </or>
        </condition>
        <emission>
            <red>1</red>
            <green>1</green>
            <blue>1</blue>
            <factor-prop>systems/electrical/dc-light</factor-prop>
        </emission>
    </animation>

    <animation>
        <name>Central CPU</name>
        <object-name>indicatorRight1</object-name>
        <type>material</type>
        <condition>
            <property>controls/lighting/test-indicator-panels</property>
        </condition>
        <emission>
            <red>1</red>
            <green>1</green>
            <blue>1</blue>
            <factor-prop>systems/electrical/dc-light</factor-prop>
        </emission>
    </animation>

    <animation>
        <name>Primary data</name>
        <object-name>indicatorRight2</object-name>
        <type>material</type>
        <condition>
            <or>
                <and>
                    <or>
                        <equals>
                            <property>sim/ja37/avionics/primaryData</property>
                            <value>1</value>
                        </equals>
                        <equals>
                            <property>instrumentation/head-up-display/serviceable</property>
                            <value>0</value>
                        </equals>
                    </or>
                    <equals>
                        <property>sim/ja37/blink/ten-Hz/state</property>
                        <value>1</value>
                    </equals>
                </and>
                <property>controls/lighting/test-indicator-panels</property>
            </or>
        </condition>
        <emission>
            <red>1</red>
            <green>1</green>
            <blue>1</blue>
            <factor-prop>systems/electrical/dc-light</factor-prop>
        </emission>
    </animation>

    <animation>
        <name>Inertia navigation</name>
        <object-name>indicatorRight3</object-name>
        <type>material</type>
        <condition>
            <or>
                <and>
                    <equals>
                        <property>sim/ja37/avionics/TN</property>
                        <value>1</value>
                    </equals>
                </and>
                <property>controls/lighting/test-indicator-panels</property>
            </or>
        </condition>
        <emission>
            <red>1</red>
            <green>1</green>
            <blue>1</blue>
            <factor-prop>systems/electrical/dc-light</factor-prop>
        </emission>
    </animation>

    <animation>
        <name>Radar</name>
        <object-name>indicatorRight4</object-name>
        <type>material</type>
        <condition>
            <or>
                <and>
                    <not-equals>
                        <property>instrumentation/radar/serviceable</property>
                        <value>1</value>
                    </not-equals>
                    <equals>
                        <property>sim/ja37/blink/ten-Hz/state</property>
                        <value>1</value>
                    </equals>
                </and>
                <less-than>
                    <property>systems/electrical/outputs/dc-voltage</property>
                    <value>23</value>
                </less-than>
                <not-equals>
                    <property>fdm/jsbsim/systems/hydraulics/system1/pressure</property>
                    <value>1</value>
                </not-equals>
                <property>controls/lighting/test-indicator-panels</property>
            </or>
        </condition>
        <emission>
            <red>1</red>
            <green>1</green>
            <blue>1</blue>
            <factor-prop>systems/electrical/dc-light</factor-prop>
        </emission>
    </animation>

    <animation>
        <name>IKS</name>
        <object-name>indicatorRight5</object-name>
        <type>material</type>
        <condition>
            <property>controls/lighting/test-indicator-panels</property>
        </condition>
        <emission>
            <red>1</red>
            <green>1</green>
            <blue>1</blue>
            <factor-prop>systems/electrical/dc-light</factor-prop>
        </emission>
    </animation>

    <animation>
        <name>Alpha</name>
        <object-name>indicatorRight6</object-name>
        <type>material</type>
        <condition>
            <or>
                <and>
                    <or>
                        <and>
                            <not>
                                <property>gear/gear/position-norm</property>
                            </not>
                            <or>
                                <and>
                                    <greater-than>
                                        <property>orientation/alpha-deg</property>
                                        <value>23</value>
                                    </greater-than>
                                    <less-than>
                                        <property>instrumentation/altimeter/indicated-altitude-ft</property>
                                        <value>32808</value>
                                    </less-than>
                                </and>
                                <and>
                                    <greater-than>
                                        <property>orientation/alpha-deg</property>
                                        <value>20</value>
                                    </greater-than>
                                    <greater-than>
                                        <property>instrumentation/altimeter/indicated-altitude-ft</property>
                                        <value>32808</value>
                                    </greater-than>
                                </and>
                            </or>
                        </and>
                        <and>
                            <property>gear/gear/position-norm</property>
                            <greater-than>
                                <property>orientation/alpha-deg</property>
                                <value>18</value>
                            </greater-than>
                            <not>
                                <property>gear/gear[1]/wow</property>
                            </not>
                            <not>
                                <property>gear/gear[2]/wow</property>
                            </not>
                        </and>
                    </or>
                    <equals>
                        <property>sim/ja37/blink/ten-Hz/state</property>
                        <value>1</value>
                    </equals>
                </and>
                <property>controls/lighting/test-indicator-panels</property>
            </or>
        </condition>
        <emission>
            <red>1</red>
            <green>1</green>
            <blue>1</blue>
            <factor-prop>systems/electrical/dc-light</factor-prop>
        </emission>
    </animation>

    <animation>
        <name>Oxygen</name>
        <object-name>indicatorRight7</object-name>
        <type>material</type>
        <condition>
            <or>
                <and>
                    <equals>
                        <property>sim/ja37/avionics/oxygen</property>
                        <value>1</value>
                    </equals>
                    <equals>
                        <property>sim/ja37/blink/ten-Hz/state</property>
                        <value>1</value>
                    </equals>
                </and>
                <property>controls/lighting/test-indicator-panels</property>
            </or>
        </condition>
        <emission>
            <red>1</red>
            <green>1</green>
            <blue>1</blue>
            <factor-prop>systems/electrical/dc-light</factor-prop>
        </emission>
    </animation>

    <animation>
        <name>Outlet temperature</name>
        <object-name>indicatorRight8</object-name>
        <type>material</type>
        <condition>
            <or>
                <and>
                    <greater-than>
                        <property>fdm/jsbsim/propulsion/engine/outlet-temperature-degc</property>
                        <value>800</value>
                    </greater-than>
                    <equals>
                        <property>sim/ja37/blink/ten-Hz/state</property>
                        <value>1</value>
                    </equals>
                </and>
                <property>controls/lighting/test-indicator-panels</property>
            </or>
        </condition>
        <emission>
            <red>1</red>
            <green>1</green>
            <blue>1</blue>
            <factor-prop>systems/electrical/dc-light</factor-prop>
        </emission>
    </animation>

    <animation>
        <name>low on fuel</name>
        <object-name>indicatorRight9</object-name>
        <type>material</type>
        <condition>
            <or>
                <and>
                    <equals>
                        <property>sim/ja37/sound/fuel-low-on</property>
                        <value>1</value>
                    </equals>
                    <equals>
                        <property>sim/ja37/blink/ten-Hz/state</property>
                        <value>1</value>
                    </equals>
                </and>
                <property>controls/lighting/test-indicator-panels</property>
            </or>
        </condition>
        <emission>
            <red>1</red>
            <green>1</green>
            <blue>1</blue>
            <factor-prop>systems/electrical/dc-light</factor-prop>
        </emission>
    </animation>

    <animation>
        <name>Weapons</name>
        <object-name>indicatorRight10</object-name>
        <type>material</type>
        <condition>
            <property>controls/lighting/test-indicator-panels</property>
        </condition>
        <emission>
            <red>1</red>
            <green>1</green>
            <blue>1</blue>
            <factor-prop>systems/electrical/dc-light</factor-prop>
        </emission>
    </animation>

</PropertyList>
