<?xml version="1.0" ?> 

  <PropertyList> 
    <path>Aircraft/Instruments-3d/radio_stack/RadioStack.ac</path>
 <panel>
  <path>Models/Instruments/Radio/radio-panel.xml</path>
  <bottom-left>
    <x-m>0.001</x-m>
    <y-m>-0.08</y-m>
    <z-m>-0.135</z-m>
  </bottom-left>
  <bottom-right>
    <x-m>0.001</x-m>
    <y-m>0.08</y-m>
    <z-m>-0.135</z-m>
  </bottom-right>
  <top-left>
    <x-m>0.001</x-m>
    <y-m>-0.08</y-m>
    <z-m>0.135</z-m>
  </top-left>
 </panel>
 
<animation>
  <type>material</type> 
  <object-name>RadioStack</object-name>
  <diffuse>
   <red>0.5</red> 
   <green>0.5</green> 
   <blue>0.5</blue>
   <factor-prop>controls/lighting/instruments-norm</factor-prop>
  </diffuse>
  <emission>
   <red-prop>instrumentation/instrumentation-light/r</red-prop> 
   <green-prop>instrumentation/instrumentation-light/g</green-prop> 
   <blue-prop>instrumentation/instrumentation-light/b</blue-prop>
   <factor-prop>controls/lighting/instruments-norm</factor-prop>
  </emission>
 </animation>


    <animation>
      <type>range</type>
      <min-m>0</min-m>
      <max-m>1.5</max-m>
    </animation>

</PropertyList>
