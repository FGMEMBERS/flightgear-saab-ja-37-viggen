    <name>ind-panel-left</name>
    <offsets>
      <x-m>  0.15</x-m>
      <y-m>  0.075</y-m>
      <z-m> -0.02674</z-m>
    </offsets>
  </model>

  <model>
    <path>Aircraft/JA37/Models/Instruments/ManometerCabin/cabin-manometer.xml</path>
    <name>cabin-mano</name>
    <offsets>
      <pitch-deg>-90</pitch-deg>
      <x-m>  0.055</x-m>
      <y-m> -0.04</y-m>
      <z-m>  0</z-m>
    </offsets>
  </model>

  <animation>
    <type>scale</type>
    <object-name>cabin-mano</object-name>
    <x-offset>0.9</x-offset>
    <y-offset>0.9</y-offset>
    <z-offset>0.9</z-offset>
  </animation>

  <model>
    <path>Aircraft/JA37/Models/Instruments/ManometerBrake/brake-manometer.xml</path>
    <name>brake-mano</name>
