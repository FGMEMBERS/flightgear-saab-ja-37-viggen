  <animation>
    <type>scale</type>
    <object-name>brake-mano</object-name>
    <x-offset>0.9</x-offset>
    <y-offset>0.9</y-offset>
    <z-offset>0.9</z-offset>
  </animation>

  <model>
    <path>Aircraft/JA37/Models/Instruments/TrimPitch/pitch-trim-indicator.xml</path>
    <name>trim-pitch</name>
