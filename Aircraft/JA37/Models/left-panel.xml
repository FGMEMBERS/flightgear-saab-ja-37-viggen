      <pitch-deg>-90</pitch-deg>
      <x-m>  0.195</x-m>
      <y-m> -0.04</y-m>
      <z-m>  0</z-m>
    </offsets>
  </model>

  <animation>
    <type>scale</type>
    <object-name>trim-pitch</object-name>
    <x-offset>0.9</x-offset>
    <y-offset>0.9</y-offset>
    <z-offset>0.9</z-offset>
  </animation>

</PropertyList>